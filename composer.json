{
  "name": "lab404/laravel-auth-checker",
  "description": "Laravel Auth Checker allows you to log users authentication, devices authenticated from and lock intrusions.",
  "type": "library",
  "keywords": [
    "laravel",
    "package",
    "plugin",
    "authentication",
    "user",
    "check",
    "device",
    "ip",
    "laravel-package",
    "laravel-plugin"
  ],
  "require": {
    "php": "^8.2",
    "laravel/framework": "^11.0",
    "jenssegers/agent": "^2.6"
  },
  "require-dev": {
    "phpunit/phpunit": "^11.0",
    "orchestra/testbench": "^9.0"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Marceau Casals",
      "email": "marceau@casals.fr"
    }
  ],
  "autoload": {
    "psr-4": {
      "Lab404\\AuthChecker\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Lab404\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Lab404\\AuthChecker\\AuthCheckerServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
